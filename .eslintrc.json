{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": ["eslint:recommended", "google"],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly",
        "_": false,
        "chrome": false,
        "$": false
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "max-len": [2, 120, 2]
    }
}
